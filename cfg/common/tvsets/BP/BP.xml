<?xml version="1.0" encoding="UTF-8"?>
<tagset xmlns:xi="http://www.w3.org/2001/XInclude">
    <svn>$Id: BP.xml 7420 2016-02-10 08:17:30Z arjan $</svn>
    <name>BP</name>
    <descr>Best Practices</descr>
    <tagged-values>
        <!-- BP follows SBR requirements -->
        <xi:include href="../SBR/SBR.xml"/>
      
        <tv norm="space" id="DatumOpname">
            <name>Datum opname</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="yes">Objecttype</stereo>
                <stereo required="yes">Attribuutsoort</stereo>
                <stereo required="yes">Gegevensgroeptype</stereo>
                <stereo required="yes">Relatiesoort</stereo> 
                <stereo required="yes">Referentielijst</stereo>
                <stereo required="yes">Referentie element</stereo>
                <stereo required="yes">Union</stereo>
                <stereo required="yes">Union element</stereo>
                <stereo required="yes">Complex datatype</stereo>
                <stereo required="yes">Externe koppeling</stereo>
            </stereotypes>
        </tv> 
        <tv norm="space" validate="boolean"  rules="NOC" id="IndicatieMateriëleHistorie">
            <name>Indicatie materiële historie</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="yes">Attribuutsoort</stereo>
                <stereo required="yes">Gegevensgroeptype</stereo>
                <stereo required="yes">Relatiesoort</stereo>
            </stereotypes>
            <declared-values>
                <value>Ja</value>
                <value>Nee</value>
                <value>Zie groep</value>
            </declared-values>
        </tv>
        <tv norm="space" validate="boolean" rules="NOC" id="IndicatieFormeleHistorie">
            <name>Indicatie formele historie</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="yes">Attribuutsoort</stereo>
                <stereo required="yes">Gegevensgroeptype</stereo>
                <stereo required="yes">Relatiesoort</stereo>
            </stereotypes>
            <declared-values>
                <value>Ja</value>
                <value>Nee</value>
                <value>Zie groep</value>
            </declared-values>
        </tv>
        <tv norm="space" id="IndicatieAuthentiek">
            <name>Indicatie authentiek</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="yes">Attribuutsoort</stereo>
                <stereo required="yes">Gegevensgroeptype</stereo>
                <stereo required="yes">Relatiesoort</stereo>
            </stereotypes>
            <declared-values>
                <value>Authentiek</value>
                <value>Basisgegeven</value>
                <value>Landelijk kerngegeven</value>
                <value>Gemeentelijk kerngegeven</value>
                <value>Overig</value>
            </declared-values>
        </tv>
        <tv norm="space" id="Patroon"><!-- MIG -->
            <name>Patroon</name>
            <name>Pattern</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">Attribuutsoort</stereo>
                <stereo required="no">Referentie element</stereo>
                <stereo required="no">Union element</stereo>
                <stereo required="no">Datatype</stereo>
                <stereo required="no">Complex datatype</stereo>
                <stereo required="no">Data element</stereo>
                <stereo required="no">Union element</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" validate="boolean" id="MogelijkGeenWaarde">
            <name>Mogelijk geen waarde</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">Attribuutsoort</stereo>
                <stereo required="no">Gegevensgroeptype</stereo>
                <stereo required="no">Relatiesoort</stereo>
                <stereo required="no">Voidable</stereo>
            </stereotypes>
            <declared-values>
                <value>Ja</value>
                <value>Nee</value>
            </declared-values>
        </tv>
       
        <tv norm="space" id="DataLocatie"><!-- MIG -->
            <name>Data locatie</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">Referentielijst</stereo>
                <stereo required="no">Referentie element</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" id="Locatie">
            <name>Locatie</name><!-- MIG -->
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">External</stereo><!--TODO externals that are conceptuals do not require this tagged value -->
                <stereo required="no">View</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space" origin="system" id="Positie">
            <name>Positie</name>
            <name>Position</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">Attribuutsoort</stereo>
                <stereo required="no">Relatiesoort</stereo>
                <stereo required="no">Union element</stereo>
                <stereo required="no">Enum</stereo>
                <stereo required="no">Data element</stereo>
                <stereo required="no">Referentie element</stereo>
                <stereo required="no">Gegevensgroep compositie</stereo>
                <stereo required="no">#none</stereo> <!-- also allowed on enumeration values that have no enum stereo -->
            </stereotypes>
        </tv>
        
    </tagged-values>
</tagset>