<?xml version="1.0" encoding="UTF-8"?>
<tagset xmlns:xi="http://www.w3.org/2001/XInclude">
    <svn>$Id: KINGSIM.xml 7389 2016-01-18 15:03:45Z arjan $</svn>
    <name>KING</name>
    <tagged-values>
        <!-- KING follows BP requirements -->
        <xi:include href="../BP/BP.xml"/>
        <tv norm="space">
            <name>Herkomst definitie</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo>Objecttype</stereo>
                <stereo>Attribuutsoort</stereo>
                <stereo>Gegevensgroeptype</stereo>
                <stereo>Relatiesoort</stereo>
            </stereotypes>
        </tv>
        <tv norm="space">
            <name>Aanduiding strijdigheid/nietigheid</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo>Attribuutsoort</stereo>
                <stereo>Gegevensgroeptype</stereo>
                <stereo>Relatiesoort</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" validate="boolean">
            <name>Indicatie in onderzoek</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo>Attribuutsoort</stereo>
                <stereo>Gegevensgroeptype</stereo>
                <stereo>Relatiesoort</stereo>
            </stereotypes>
        </tv>
        <tv norm="space">
            <name>Herkomst definitie</name>
            <derive>yes</derive>
            <stereotypes>
                <!-- in addition to BP -->
                <stereo>Referentielijst</stereo> 
                <stereo>Referentie element</stereo>
                <stereo>Union</stereo>
                <stereo>Union element</stereo>
                <stereo>Complex datatype</stereo>
                <stereo>Data element</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space">
            <name>Beheerder</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo>External</stereo>
                <stereo>View</stereo>
            </stereotypes>
        </tv>
        
        <tv norm="space">
            <name>Groepnaam</name>
            <derive>no</derive>
            <stereotypes>
                <stereo>Tekentechnisch</stereo>
            </stereotypes>
        </tv>

    </tagged-values>
</tagset>