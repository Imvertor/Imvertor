<?xml version="1.0" encoding="UTF-8"?>
<tagset>
    <svn>$Id: SBR.xml 7420 2016-02-10 08:17:30Z arjan $</svn>
    <name>SBR</name>
    <descr>Stelsel basisregistraties</descr>
    <web>http://stelselcatalogus.logius.nl/over-de-stelselcatalogus/metadata/</web>
    
    <tagged-values xml:id="tagset">
        <tv norm="space" id="Herkomst">
            <name>Herkomst</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="yes">Objecttype</stereo>
                <stereo required="yes">Complex datatype</stereo>
                <stereo required="yes">Data element</stereo>
                <stereo required="yes">Attribuutsoort</stereo>
                <stereo required="yes">Relatiesoort</stereo>
                <stereo required="yes">Gegevensgroeptype</stereo>
                <stereo required="yes">Referentielijst</stereo>
                <stereo required="yes">Referentie element</stereo>
                <stereo required="yes">Union</stereo>
                <stereo required="yes">Union element</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="HerkomstDefinitie">
            <name>Herkomst definitie</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="yes">Objecttype</stereo>
                <stereo required="yes">Attribuutsoort</stereo>
                <stereo required="yes">Gegevensgroeptype</stereo>
                <stereo required="yes">Relatiesoort</stereo>
            </stereotypes>
        </tv>
        
        <!-- TODO Vaststellen per tagged value of deze derived moeten worden of niet. -->
        <tv norm="space" id="Eigenaar">
            <name>Eigenaar</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">Objecttype</stereo> <!-- TODO anders dan metamodel -->
                <stereo required="no">Attribuutsoort</stereo> <!-- TODO anders dan metamodel -->
                <stereo required="no">Gegevensgroeptype</stereo> <!-- TODO anders dan metamodel -->
            </stereotypes>
        </tv>
        <tv norm="note" id="Populatie">
            <name>Populatie</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">Objecttype</stereo>
            </stereotypes>
        </tv>
        <tv norm="space" id="Kwaliteit">
            <name>Kwaliteitsbegrip</name>
            <name>Kwaliteit</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">Objecttype</stereo>
            </stereotypes>
        </tv>
        <!-- Authentiek wel onderdeel van SBR, maar niet in metamodel.
        <tv norm="note" validate="boolean" id="Authentiek">
            <name>Authentiek</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="yes">Attribuutsoort</stereo>
                <stereo required="yes">Gegevensgroeptype</stereo>
                <stereo required="yes">Relatiesoort</stereo>
            </stereotypes>
        </tv>
        -->
        <tv norm="note" id="Regels">
            <name>Regels</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">Attribuutsoort</stereo>
                <stereo required="no">Gegevensgroeptype</stereo>
                <stereo required="no">Relatiesoort</stereo>
            </stereotypes>
        </tv>
        <tv norm="note" id="Toelichting">
            <name>Toelichting</name>
            <derive>yes</derive>
            <stereotypes>
                <stereo required="no">Objecttype</stereo>
                <stereo required="no">Attribuutsoort</stereo>
                <stereo required="no">Gegevensgroeptype</stereo>
                <stereo required="no">Relatiesoort</stereo>
                <stereo required="no">Referentielijst</stereo>
                <stereo required="no">Referentie element</stereo>
            </stereotypes>
        </tv> 
    </tagged-values>
</tagset>