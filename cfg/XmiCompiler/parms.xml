<?xml version="1.0" encoding="UTF-8"?>
<config>
   <id>
      <name>XmiCompiler parameters</name>
      <version-control>$Id: parms.xml 7361 2016-01-06 12:06:30Z arjan $</version-control>
   </id>
   <cli-parms>
      <!-- none -->
   </cli-parms>

   <properties>

      <!--  location of xmi / Imvert conversion results -->
      <WORK_XMI_MASTER_FILE>${cli/managedinputfolder}/xmi/export.xmi</WORK_XMI_MASTER_FILE>
      <WORK_IMVERTOR_BASE_FOLDER>${system/work-folder-path}/imvert</WORK_IMVERTOR_BASE_FOLDER>
      <WORK_PROPERTIESDUMP_FILE>${system/work-folder-path}/properties.xml</WORK_PROPERTIESDUMP_FILE>

      <WORK_XMI_FOLDER>${system/work-folder-path}/xmi</WORK_XMI_FOLDER>

      <!--  template EAP used as the basis for all working copies -->
      <TEMPLATE_TEMPLATE_FILE>${cli/managedinputfolder}/eap/TEMPLATE.EAP</TEMPLATE_TEMPLATE_FILE>
      <TEMPLATE_TEMPLATE_FILE_GUID>${cli/managedinputfolder}/eap/TEMPLATE.EAP.GUID</TEMPLATE_TEMPLATE_FILE_GUID>

      <!-- path to compacter -->
      <XMI_COMPACT_XSLPATH>XMI1Compact.xsl</XMI_COMPACT_XSLPATH>
 
   </properties>

</config>
