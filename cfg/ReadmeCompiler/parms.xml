<?xml version="1.0" encoding="UTF-8"?>
<config>
   <id>
      <name>Readme compiler</name>
      <version-control>$Id: parms.xml 7268 2015-09-20 10:21:54Z arjan $</version-control>
   </id>
   <cli-parms>
      <cli-parm>
         <name>contactemail</name>
         <arg>email-address</arg>
         <tip>The email address for support on the (form of the) distributed release</tip>
         <required>false</required>
      </cli-parm> 
      <cli-parm>
         <name>contacturl</name>
